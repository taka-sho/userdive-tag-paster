info
  table(border="1")
    tr
      th(each="{title in rawdata}") {title}
    tr
      th(each="{value in rawdata.value}") {value}
  script.
    /*console.log(opts.status)*/
    var rawData  = JSON.parse(
      document.getElementsByTagName("info")[0].getAttribute("status")
      )
    this.rawdata = rawData
