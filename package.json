    "gulp-load-plugins": "^1.5.0",
    "gulp-size": "^3.0.0",
    "gulp-zip": "^4.1.0",
    "husky": "^0.14.3",
    "imagemin-cli": "^3.0.0",
    "jest": "22.2.1",
    "lint-staged": "^6.1.0",
    "main-bower-files": "^2.13.1",
    "mock-local-storage": "^1.0.5",
