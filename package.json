    "lint:flow": "flow",
    "lint:js": "eslint . --cache",
    "lint:pug": "pug-lint ./app/*.pug",
    "prebuild": "npm run clean",
    "release": "gulp package",
    "tag": "uglifyjs app/scripts.babel/tag.js -m",
