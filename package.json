  },
  "homepage": "https://github.com/uncovertruth/userdive-tag-paster#readme",
  "keywords": [],
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write",
      "eslint --cache",
      "git add"
    ],
    "*.css": [
      "stylefmt",
      "stylelint",
      "git add"
    ],
    "*.pug": [
      "pug-lint"
    ]
  },
  "main": "n/a",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uncovertruth/userdive-tag-paster.git"
  },
  "scripts": {
    "b:gulp": "gulp build",
    "b:images": "imagemin app/images/*.png --out-dir=dist/images/",
    "b:pug": "pug app/*.pug --out dist/",
    "b:webpack": "cross-env NODE_ENV=production webpack",
    "build": "run-s clean b:pug b:webpack b:images b:gulp",
    "clean": "find app -name '*.html' -delete && rimraf dist app/scripts package",
    "coverage": "codecov -t $CODECOV_TOKEN -f ./coverage/*/lcov.info",
    "lint": "run-p lint:*",
    "lint:css": "stylelint app/styles/*.css",
    "lint:flow": "flow",
    "lint:js": "eslint . --cache",
    "lint:pug": "pug-lint ./app/*.pug",
    "precommit": "lint-staged",
    "release": "gulp package",
    "tag": "uglifyjs app/js/tag.js -m",
    "test": "karma start --single-run"
  }
}
