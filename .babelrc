{
  "presets": ["env", "flow"],
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ],
  "env": {
    "development": {
      "presets": [
        "power-assert",
      ],
      "plugins": [
        "istanbul"
      ]
    }
  }
}
